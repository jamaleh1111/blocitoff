<tr id="buttons">
  <td><%= item.name %></td>
  <td><%= distance_of_time_in_words_to_now(item.expires_at) %></td>
  <td><%= link_to "", user_item_path(current_user, item), method: :delete,  class: 'glyphicon glyphicon-trash' %></td>
  <td><%= link_to user_item_complete_path(current_user, item), method: :put do %><span class="glyphicon glyphicon-check"></span></td>
  <% end %>
</tr>
